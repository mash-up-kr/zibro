{
  "name": "zibro",
  "version": "0.2.0",
  "description": "막차 시간을 넘긴 청춘들을 위한 교통비 최적화 정보 제공 서비스",
  "author": "chayeoi",
  "license": "MIT",
  "homepage": "https://zibro.netlify.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mash-up-kr/zibro.git"
  },
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "release": "standard-version"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "git add"
    ]
  },
  "dependencies": {
    "@craco/craco": "^5.2.4",
